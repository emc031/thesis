@article{CLOSE1984209,
title = "The ratio $\Gamma(B\to Dl\nu)/\Gamma(B\to D^* l\nu)$ in the quark model",
journal = "Physics Letters B",
volume = "149",
number = "1",
pages = "209 - 212",
year = "1984",
issn = "0370-2693",
doi = "https://doi.org/10.1016/0370-2693(84)91586-7",
url = "http://www.sciencedirect.com/science/article/pii/0370269384915867",
author = "F.E. Close and G.J. Gounaris and J.E. Paschalis",
abstract = "Using the non-relativistic quark model and including perturbative QCD corrections to first non-trivial order, we find that exclusive semileptonic decays of bottom mesons satisfy 0.3 < Γ(B → Dℓν)/Γ(B → D∗ℓν) < 0.5. For charmed meson decays, no corresponding prediction can be made for the ratio Γ(D → Kℓν)/Γ(D → K∗ℓν) other than that K dominates over K∗, whereas for top decays B∗ dominates over B."
}